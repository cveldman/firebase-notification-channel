{
    "name": "veldman/firebase-notification-channel",
    "description": "Firebase notification channel for Laravel.",
    "keywords": ["laravel", "notifications", "firebase"],
    "type": "library",
    "authors": [
        {
            "name": "Corn√© Veldman",
            "email": "cveldman@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/notifications": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Veldman\\Firebase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veldman\\Firebase\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminate\\Notifications\\FirebaseServiceProvider"
            ]
        }
    }
}
